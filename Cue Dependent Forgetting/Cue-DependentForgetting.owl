        <rdfs:label xml:lang="en">BiasedAgent</rdfs:label>
        <terms:description xml:lang="en">Represents an entity that experiences the bias.</terms:description>
    </owl:Class>
    


    <!-- https://github.com/ranacoskun/cognitive_bias_ontology#CognitiveBias -->

    <owl:Class rdf:about="https://github.com/ranacoskun/cognitive_bias_ontology#CognitiveBias">
        <rdfs:label xml:lang="en">CognitiveBias</rdfs:label>
        <terms:description xml:lang="en">Represents the overarching concept of cognitive biases.</terms:description>
    </owl:Class>
    


    <!-- https://github.com/ranacoskun/cognitive_bias_ontology#CueDependentForgetting -->

    <owl:Class rdf:about="https://github.com/ranacoskun/cognitive_bias_ontology#CueDependentForgetting">
        <rdfs:subClassOf rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology#CognitiveBias"/>
        <rdfs:label xml:lang="en">CueDependentForgetting</rdfs:label>
        <terms:description xml:lang="en">A specific cognitive bias where the ability to recall information is dependent on cues present at the time of encoding and at the time of retrieval.</terms:description>
    </owl:Class>
    


    <!-- https://github.com/ranacoskun/cognitive_bias_ontology#Reproducing -->

    <owl:Class rdf:about="https://github.com/ranacoskun/cognitive_bias_ontology#Reproducing">
        <rdfs:label xml:lang="en">Reproducing</rdfs:label>
        <terms:description xml:lang="en">Models the process of recalling and possibly reconstructing memories. This class deals with how an agent processes and reproduces information from memory.</terms:description>
    </owl:Class>
    


    <!-- https://github.com/ranacoskun/cognitive_bias_ontology#Trigger -->

    <owl:Class rdf:about="https://github.com/ranacoskun/cognitive_bias_ontology#Trigger">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.cidoc-crm.org/html/cidoc_crm_v7.1.3.html#hasType"/>
                <owl:hasValue xml:lang="en">context-dependent</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.cidoc-crm.org/html/cidoc_crm_v7.1.3.html#hasType"/>
                <owl:hasValue xml:lang="en">state-dependent</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Trigger</rdfs:label>
        <terms:description xml:lang="en">Represents an environmental or contextual stimulus that facilitates the recall of memories by a BiasedAgent. Has types depending on the provenance of the trigger: context-dependent (referring to the environment) and state-dependent (referring to the person experiencing the bias).</terms:description>
    </owl:Class>
    


    <!-- https://w3id.org/framester/data/framestercore/Information -->

    <owl:Class rdf:about="https://w3id.org/framester/data/framestercore/Information">
        <rdfs:label xml:lang="en">Information</rdfs:label>
        <terms:description xml:lang="en">Describes the information which the person would like to memorize.</terms:description>
    </owl:Class>
    


    <!-- https://w3id.org/framester/data/framestercore/Memorization -->

    <owl:Class rdf:about="https://w3id.org/framester/data/framestercore/Memorization">
        <rdfs:label xml:lang="en">Memorization</rdfs:label>
        <terms:description xml:lang="en">Models memorizing activity.</terms:description>
    </owl:Class>
    


    <!-- https://w3id.org/framester/data/framestercore/People -->

    <owl:Class rdf:about="https://w3id.org/framester/data/framestercore/People">
