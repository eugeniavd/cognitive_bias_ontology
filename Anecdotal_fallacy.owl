    <!-- https://w3id.org/framester/data/framestercore/Activity -->

    <owl:Class rdf:about="https://w3id.org/framester/data/framestercore/Activity">
        <owl:disjointWith rdf:resource="https://w3id.org/framester/data/framestercore/ExperiencerObj"/>
        <owl:disjointWith rdf:resource="https://w3id.org/framester/data/framestercore/Pattern"/>
