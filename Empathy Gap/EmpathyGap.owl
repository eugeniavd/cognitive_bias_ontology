        <rdfs:isDefinedBy>https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/cbi.owl#BiasedAgent</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">BiasedAgent</rdfs:label>
    </owl:Class>
    


