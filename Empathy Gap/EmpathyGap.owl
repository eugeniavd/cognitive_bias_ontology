        <rdfs:isDefinedBy>https://delicias.dia.fi.upm.es/ontologies/ObjectWithStates.owl#State</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">State</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/Empathy%20Gap/EmpathyGap.owlEmpathyGap -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/Empathy%20Gap/EmpathyGap.owlEmpathyGap">
