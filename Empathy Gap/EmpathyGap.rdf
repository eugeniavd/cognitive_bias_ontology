     -->

    


    <!-- https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#low -->

    <owl:DatatypeProperty rdf:about="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#low">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/parameter.owl#hasParameterDataValue"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/framester/data/framestercore/Accuracy"/>
            </owl:Restriction>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#negative -->

    <owl:DatatypeProperty rdf:about="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#negative">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/parameter.owl#hasParameterDataValue"/>
                <owl:someValuesFrom rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#PhysicalState"/>
                <owl:someValuesFrom rdf:resource="https://softeng.polito.it/rsctx/#d4e1858#EmotionalState"/>
            </owl:Restriction>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#positive -->

    <owl:DatatypeProperty rdf:about="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#positive">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/parameter.owl#hasParameterDataValue"/>
                <owl:someValuesFrom rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#PhysicalState"/>
                <owl:someValuesFrom rdf:resource="https://softeng.polito.it/rsctx/#d4e1858#EmotionalState"/>
            </owl:Restriction>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://delicias.dia.fi.upm.es/ontologies/ObjectWithStates.owl#State -->

    <owl:Class rdf:about="https://delicias.dia.fi.upm.es/ontologies/ObjectWithStates.owl#State">
        <rdfs:isDefinedBy>https://delicias.dia.fi.upm.es/ontologies/ObjectWithStates.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">State</rdfs:label>
        <terms:description xml:lang="en">States are the different states that an object can have. States must belong to a single collection of non-duplicate elements (i.e., to a set).</terms:description>
    </owl:Class>
    


    <!-- https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#BiasedAgent -->

    <owl:Class rdf:about="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#BiasedAgent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/framester/data/framestercore/People"/>
        <owl:disjointWith rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#NonBiasedAgent"/>
        <rdfs:isDefinedBy>https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#BiasedAgent</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">BiasedAgent</rdfs:label>
        <terms:description xml:lang="en">Represents an entity that experiences the bias.</terms:description>
    </owl:Class>
    


    <!-- https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#CognitiveBias -->

    <owl:Class rdf:about="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#CognitiveBias">
        <owl:disjointWith rdf:resource="https://w3id.org/framester/data/framestercore/Accuracy"/>
        <rdfs:isDefinedBy>https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#CognitiveBias</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">CognitiveBias</rdfs:label>
        <terms:description xml:lang="en">A general class representing cognitive biases.</terms:description>
    </owl:Class>
    


    <!-- https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#EmpathyGap -->

    <owl:Class rdf:about="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#EmpathyGap">
        <rdfs:subClassOf rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#CognitiveBias"/>
        <rdfs:isDefinedBy>https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#EmpathyGap</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">EmpathyGap</rdfs:label>
        <terms:description xml:lang="en">A cognitive bias where a person underestimates the influence of different emotional or physical states on their decisions or behaviors.</terms:description>
    </owl:Class>
    


    <!-- https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#NonBiasedAgent -->

    <owl:Class rdf:about="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#NonBiasedAgent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/framester/data/framestercore/People"/>
        <rdfs:isDefinedBy>https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#NonBiasedAgent</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">NonBiasedAgent</rdfs:label>
        <terms:description xml:lang="en">Represents an idealized entity that makes decisions based on objective analysis, facts, and logical reasoning, without being affected by cognitive biases.</terms:description>
    </owl:Class>
    


    <!-- https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#PhysicalState -->

    <owl:Class rdf:about="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#PhysicalState">
        <rdfs:subClassOf rdf:resource="https://delicias.dia.fi.upm.es/ontologies/ObjectWithStates.owl#State"/>
        <rdfs:isDefinedBy>https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#PhysicalState</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">PhysicalState</rdfs:label>
        <terms:description xml:lang="en">Refers to the current condition of an entity's body, encompassing aspects like health, energy levels, fatigue, and overall physical well-being. Has a type (predefined string as &quot;positive&quot;, &quot;negative&quot;, etc.)</terms:description>
    </owl:Class>
    


    <!-- https://softeng.polito.it/rsctx/#d4e1858#EmotionalState -->

