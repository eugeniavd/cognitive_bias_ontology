<?xml version="1.0"?>
<rdf:RDF xmlns="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#"
     xml:base="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main"
     xmlns:fs="https://w3id.org/framester/data/framestercore/"
     xmlns:cbi="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#"
     xmlns:crm="https://www.cidoc-crm.org/html/cidoc_crm_v7.1.3.html#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:obst="https://delicias.dia.fi.upm.es/ontologies/ObjectWithStates.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:rsctx="https://softeng.polito.it/rsctx/#d4e1858#"
     xmlns:terms="https://www.dublincore.org/specifications/dublin-core/dcmi-terms/"
     xmlns:reaction="http://www.ontologydesignpatterns.org/cp/owl/reaction.owl.v0.1#"
     xmlns:parameter="http://www.ontologydesignpatterns.org/cp/owl/parameter.owl#"
     xmlns:classification="http://www.ontologydesignpatterns.org/cp/owl/classification.owl#">
    <owl:Ontology rdf:about="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#">
        <rdf:about>https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#</rdf:about>
        <rdfs:comment xml:lang="en">The "empathy gap" refers to a cognitive bias where people underestimate the influences of visceral drives on their own attitudes, preferences, and behaviors. Typically, this gap manifests when a person is in one state (e.g., calm, content) and fails to accurately predict their behavior or decisions when in a different state (e.g., angry, hungry, in pain). This can also apply when judging others&apos; actions without considering their emotional or physical state, leading to misunderstandings or misjudgments.</rdfs:comment>
        <rdfs:isDefinedBy>https://github.com/ranacoskun/cognitive_bias_ontology/tree/main/Empathy%20Gap</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Empathy Gap module of the Cognitive Bias Ontology</rdfs:label>
        <hasComponent>http://www.ontologydesignpatterns.org/cp/owl/classification.owl#</hasComponent>
        <hasComponent>http://www.ontologydesignpatterns.org/cp/owl/parameter.owl#</hasComponent>
        <hasComponent>http://www.ontologydesignpatterns.org/cp/owl/reaction.owl.v0.1#</hasComponent>
        <hasComponent>https://delicias.dia.fi.upm.es/ontologies/ObjectWithStates.owl</hasComponent>
        <hasComponent>https://softeng.polito.it/rsctx/#d4e1858</hasComponent>
        <hasComponent>https://w3id.org/framester/data/framestercore/</hasComponent>
        <hasComponent>https://www.cidoc-crm.org/html/cidoc_crm_v7.1.3.html#</hasComponent>
        <terms:created>19/5/2024</terms:created>
        <terms:creator xml:lang="en">Evgeniia Vdovichenko</terms:creator>
        <terms:description xml:lang="en">Empathy Gap Bias specification of Cognitive Bias Ontology</terms:description>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/1999/02/22-rdf-syntax-ns#about -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#about"/>
    


    <!-- https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#hasComponent -->

    <owl:AnnotationProperty rdf:about="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#hasComponent"/>
    


    <!-- https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#userstory -->

    <owl:AnnotationProperty rdf:about="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#userstory"/>
    


    <!-- https://www.dublincore.org/specifications/dublin-core/dcmi-terms/created -->

    <owl:AnnotationProperty rdf:about="https://www.dublincore.org/specifications/dublin-core/dcmi-terms/created"/>
    


    <!-- https://www.dublincore.org/specifications/dublin-core/dcmi-terms/creator -->

    <owl:AnnotationProperty rdf:about="https://www.dublincore.org/specifications/dublin-core/dcmi-terms/creator"/>
    


    <!-- https://www.dublincore.org/specifications/dublin-core/dcmi-terms/description -->

    <owl:AnnotationProperty rdf:about="https://www.dublincore.org/specifications/dublin-core/dcmi-terms/description"/>
    


