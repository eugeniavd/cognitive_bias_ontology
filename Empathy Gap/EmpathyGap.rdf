    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/classification.owl#isClassifiedBy -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/classification.owl#isClassifiedBy">
        <rdfs:domain rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#BiasedAgent"/>
        <rdfs:domain rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#EmpathyGap"/>
        <rdfs:domain rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#NonBiasedAgent"/>
        <rdfs:domain rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#PhysicalState"/>
        <rdfs:domain rdf:resource="https://softeng.polito.it/rsctx/#d4e1858#EmotionalState"/>
        <rdfs:range rdf:resource="https://delicias.dia.fi.upm.es/ontologies/ObjectWithStates.owl#State"/>
        <rdfs:range rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#CognitiveBias"/>
        <rdfs:range rdf:resource="https://w3id.org/framester/data/framestercore/People"/>
        <rdf:about>http://ontologydesignpatterns.org/wiki/Submissions:Classification</rdf:about>
        <rdfs:isDefinedBy>http://www.ontologydesignpatterns.org/cp/owl/classification.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">isClassifiedBy</rdfs:label>
        <terms:description xml:lang="en">To represent the relations between concepts (roles, task, parameters) and entities (person, events, values), which concepts can be assigned to. To formalize the application (e.g. tagging) of informal knowledge organization systems such as lexica, thesauri, subject directories, folksonomies, etc., where concepts are first-order elements.</terms:description>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/parameter.owl#isParameterFor -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/parameter.owl#isParameterFor">
        <owl:inverseOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/parameter.owl#hasParameter"/>
        <rdfs:domain rdf:resource="https://w3id.org/framester/data/framestercore/Accuracy"/>
        <rdfs:range rdf:resource="https://w3id.org/framester/data/framestercore/Predicting"/>
        <rdf:about>http://ontologydesignpatterns.org/wiki/Submissions:Parameter</rdf:about>
        <rdfs:isDefinedBy>http://www.ontologydesignpatterns.org/cp/owl/parameter.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">isParameterFor</rdfs:label>
        <terms:description xml:lang="en">A Concept can have a Parameter that constrains the attributes that a classified Entity can have in a certain Situation, e.g. a 4WheelDriver Role definedIn the ItalianTrafficLaw has a MinimumAge parameter on the Amount 16</terms:description>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/parameter.owl#hasParameter -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/parameter.owl#hasParameter">
        <rdfs:domain rdf:resource="https://w3id.org/framester/data/framestercore/Predicting"/>
        <rdfs:range rdf:resource="https://w3id.org/framester/data/framestercore/Accuracy"/>
        <rdf:about>http://ontologydesignpatterns.org/wiki/Submissions:Parameter</rdf:about>
        <rdfs:isDefinedBy>http://www.ontologydesignpatterns.org/cp/owl/parameter.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasParameter</rdfs:label>
        <terms:description xml:lang="en">A Concept can have a Parameter that constrains the attributes that a classified Entity can have in a certain Situation, e.g. a 4WheelDriver Role definedIn the ItalianTrafficLaw has a MinimumAge parameter on the Amount 16.</terms:description>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/parameter.owl#hasParameterDataValue -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/parameter.owl#hasParameterDataValue">
        <rdfs:domain rdf:resource="https://w3id.org/framester/data/framestercore/Accuracy"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#low"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:range>
        <rdf:about>http://ontologydesignpatterns.org/wiki/Submissions:Parameter</rdf:about>
        <rdfs:isDefinedBy>http://www.ontologydesignpatterns.org/cp/owl/parameter.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasParameterDataValue</rdfs:label>
        <terms:description xml:lang="en">Parametrizes values from a datatype. For example, a Parameter AgeForDriving hasParameterDataValue 18 on datatype xsd:int, in the Italian traffic code. In this example, AgeForDriving isDefinedIn the Norm ItalianTrafficCodeAgeDriving.</terms:description>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/reaction.owl.v0.1#isPerformedBy -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/reaction.owl.v0.1#isPerformedBy">
        <owl:inverseOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/reaction.owl.v0.1#performs"/>
        <rdfs:domain rdf:resource="https://w3id.org/framester/data/framestercore/Deciding"/>
        <rdfs:domain rdf:resource="https://w3id.org/framester/data/framestercore/Predicting"/>
        <rdfs:range rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#BiasedAgent"/>
        <rdfs:range rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#NonBiasedAgent"/>
        <rdf:about>http://ontologydesignpatterns.org/wiki/Submissions:Reaction</rdf:about>
        <rdfs:isDefinedBy>http://www.ontologydesignpatterns.org/cp/owl/reaction.owl.v0.1</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">isPerformedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/reaction.owl.v0.1#performs -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/reaction.owl.v0.1#performs">
        <rdfs:domain rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#BiasedAgent"/>
        <rdfs:domain rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#NonBiasedAgent"/>
        <rdfs:range rdf:resource="https://w3id.org/framester/data/framestercore/Deciding"/>
        <rdfs:range rdf:resource="https://w3id.org/framester/data/framestercore/Predicting"/>
        <rdf:about>http://ontologydesignpatterns.org/wiki/Submissions:Reaction</rdf:about>
        <rdfs:isDefinedBy>http://www.ontologydesignpatterns.org/cp/owl/reaction.owl.v0.1</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">performs</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://delicias.dia.fi.upm.es/ontologies/ObjectWithStates.owl#isStateOf -->

    <owl:ObjectProperty rdf:about="https://delicias.dia.fi.upm.es/ontologies/ObjectWithStates.owl#isStateOf">
        <owl:inverseOf rdf:resource="https://delicias.dia.fi.upm.es/ontologies/ObjectWithStates.owl# hasState"/>
        <rdfs:domain rdf:resource="https://delicias.dia.fi.upm.es/ontologies/ObjectWithStates.owl#State"/>
        <rdfs:range rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#BiasedAgent"/>
        <rdf:about>http://ontologydesignpatterns.org/wiki/Submissions:Object_with_states</rdf:about>
        <rdfs:isDefinedBy>https://delicias.dia.fi.upm.es/ontologies/ObjectWithStates.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">isStateOf</rdfs:label>
        <terms:description xml:lang="en">Defines the object that has an state</terms:description>
    </owl:ObjectProperty>
    


    <!-- https://delicias.dia.fi.upm.es/ontologies/ObjectWithStates.owl#hasState -->

    <owl:ObjectProperty rdf:about="https://delicias.dia.fi.upm.es/ontologies/ObjectWithStates.owl#hasState">
        <rdfs:domain rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#BiasedAgent"/>
        <rdfs:range rdf:resource="https://delicias.dia.fi.upm.es/ontologies/ObjectWithStates.owl#State"/>
        <rdf:about>http://ontologydesignpatterns.org/wiki/Submissions:Object_with_states</rdf:about>
        <rdfs:isDefinedBy>https://delicias.dia.fi.upm.es/ontologies/ObjectWithStates.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasState</rdfs:label>
        <terms:description xml:lang="en">Defines the state of an object.</terms:description>
    </owl:ObjectProperty>
    


    <!-- https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#affectedBy -->

    <owl:ObjectProperty rdf:about="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#affectedBy">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#BiasedAgent"/>
        <rdfs:domain rdf:resource="https://w3id.org/framester/data/framestercore/Predicting"/>
        <rdfs:range rdf:resource="https://delicias.dia.fi.upm.es/ontologies/ObjectWithStates.owl#State"/>
        <rdfs:range rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#CognitiveBias"/>
        <rdfs:isDefinedBy>https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#affectedBy</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">affectedBy</rdfs:label>
        <terms:description xml:lang="en">Indicates the influence of the cognitive bias to the individual whose perception is influenced by or belief which is influenced by the perception experience.</terms:description>
    </owl:ObjectProperty>
    


    <!-- https://www.cidoc-crm.org/html/cidoc_crm_v7.1.3.html#hasType -->

    <owl:ObjectProperty rdf:about="https://www.cidoc-crm.org/html/cidoc_crm_v7.1.3.html#hasType">
        <rdfs:domain rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#PhysicalState"/>
        <rdfs:domain rdf:resource="https://softeng.polito.it/rsctx/#d4e1858#EmotionalState"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#negative"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://github.com/ranacoskun/cognitive_bias_ontology/tree/main#positive"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:range>
        <rdfs:isDefinedBy>https://cidoc-crm.org/html/cidoc_crm_v7.1.3.html#P2</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasType</rdfs:label>
        <terms:description xml:lang="en">This property allows sub-typing of entities – a form of specialization – through the use of a terminological hierarchy, or thesaurus.</terms:description>
    </owl:ObjectProperty>
    


    <!-- https://www.cidoc-crm.org/html/cidoc_crm_v7.1.3.html#isAbout -->

    <owl:ObjectProperty rdf:about="https://www.cidoc-crm.org/html/cidoc_crm_v7.1.3.html#isAbout">
        <rdfs:domain rdf:resource="https://w3id.org/framester/data/framestercore/Predicting"/>
        <rdfs:range rdf:resource="https://w3id.org/framester/data/framestercore/Deciding"/>
        <rdfs:isDefinedBy>https://cidoc-crm.org/html/cidoc_crm_v7.1.3.html#P129</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">isAbout</rdfs:label>
        <terms:description xml:lang="en">This property documents that an instance of E89 Propositional Object has as subject an instance of E1 CRM Entity. This differs from P67 refers to (is referred to by), which refers to an instance of E1 CRM Entity, in that it describes the primary subject or subjects of an instance of E89 Propositional Object.</terms:description>
    </owl:ObjectProperty>
    


