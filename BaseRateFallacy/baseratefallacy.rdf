    <!-- http://ontologydesignpatterns.org/cp/owl/parameter.owlhas#ParameterDataValue -->

    <owl:DatatypeProperty rdf:about="http://ontologydesignpatterns.org/cp/owl/parameter.owlhas#ParameterDataValue">
        <rdfs:domain rdf:resource="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#BaseRate"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment xml:lang="en">Parametrizes values from a datatype</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">http://ontologydesignpatterns.org/cp/owl/parameter.owl</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">hasParameterDataValue</rdfs:label>
    </owl:DatatypeProperty>
    


