    <!-- https://modellingdh.github.io/ont/odp/term/Observation -->

    <owl:Class rdf:about="https://modellingdh.github.io/ont/odp/term/Observation">
        <rdfs:comment xml:lang="en">The product of an act of scrutiny which may or may not have been performed with a critical disposition</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">https://modellingdh.github.io/ont/odp/term/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Observation</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#BaseRate -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#BaseRate">
        <rdfs:comment xml:lang="en">Base rate refers to the parameter that Base Rate Information has</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#BaseRate</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">BaseRate</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#BaseRateInformation -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#BaseRateInformation">
        <rdfs:subClassOf rdf:resource="https://w3id.org/framester/data/framestercore/Information"/>
        <owl:disjointWith rdf:resource="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#SpecificInformation"/>
        <rdfs:comment xml:lang="en">Base rate information refers to the type of information that is the percentage of a population that has a specific characteristic</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#BaseRateInformation</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">BaseRateInformation</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#SpecificInformation -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#SpecificInformation">
        <rdfs:subClassOf rdf:resource="https://w3id.org/framester/data/framestercore/Information"/>
        <rdfs:comment xml:lang="en">Specific information refers to the type of information that is case-specific</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#SpecificInformation</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">SpecificInformation</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/cbi.owl#BaseRateFallacy -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/cbi.owl#BaseRateFallacy">
        <rdfs:subClassOf rdf:resource="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/cbi.owl#CognitiveBias"/>
        <rdfs:comment xml:lang="en">The base rate fallacy illustrates how neglecting the overall context or base rate information can lead to inaccurate perceptions and decisions.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/cbi.owl#BaseRateFallacy</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">BaseRateFallacy</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/cbi.owl#BiasedAgent -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/cbi.owl#BiasedAgent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/framester/data/framestercore/People"/>
        <rdfs:comment xml:lang="en">Represents an entity that experiences the bias.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/cbi.owl#BiasedAgent</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">BiasedAgent</rdfs:label>
    </owl:Class>
    


    <!-- https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/cbi.owl#CognitiveBias -->

    <owl:Class rdf:about="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/cbi.owl#CognitiveBias">
        <rdfs:comment xml:lang="en">Represents the overarching concept of cognitive biases.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/cbi.owl#CognitiveBias</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">CognitiveBias</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/framester/data/framestercore/Assessing -->

    <owl:Class rdf:about="https://w3id.org/framester/data/framestercore/Assessing">
        <rdfs:comment xml:lang="en">An Assessor examines a Phenomenon to figure out its Value according to some Feature of the Phenomenon. This Value is a factor in determining the acceptability of the Phenomenon.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">https://w3id.org/framester/data/framestercore/Assessing</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Assessing</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/framester/data/framestercore/Deciding -->

    <owl:Class rdf:about="https://w3id.org/framester/data/framestercore/Deciding">
        <rdfs:comment xml:lang="en">A Cognizer makes a Decision, which may be about an entity or a course of action.</rdfs:comment>
        <rdfs:isDefinedBy>https://w3id.org/framester/data/framestercore/Deciding</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">Deciding</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/framester/data/framestercore/Information -->

    <owl:Class rdf:about="https://w3id.org/framester/data/framestercore/Information">
        <rdfs:comment xml:lang="en">A Cognizer knows or comes to know some piece of Information about a Topic. In this frame, many LUs encode a specific Means_of_Gathering and/or Source, but these may also be expressed separately</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">https://w3id.org/framester/data/framestercore/Information</rdfs:isDefinedBy>
        <rdfs:label>Information</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/framester/data/framestercore/People -->

    <owl:Class rdf:about="https://w3id.org/framester/data/framestercore/People">
        <rdfs:comment xml:lang="en">Denotes a human being and express that we are talking about human cognitive biases.</rdfs:comment>
        <rdfs:isDefinedBy>https://w3id.org/framester/data/framestercore/People</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">People</rdfs:label>
    </owl:Class>
    


    <!-- 
