    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#DiseaseBaseRateInformation -->

    <owl:NamedIndividual rdf:about="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#DiseaseBaseRateInformation">
        <rdf:type rdf:resource="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#BaseRateInformation"/>
        <parameter:hasParameter rdf:resource="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#Rate"/>
        <rdfs:comment xml:lang="en">Information regarding the prevalence of a specific disease within a population proving that the result might be a false positive, used as a base rate for evaluation purposes.</rdfs:comment>
        <rdfs:label xml:lang="en">Disease Base Rate Information</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#DrLee -->

    <owl:NamedIndividual rdf:about="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#DrLee">
        <rdf:type rdf:resource="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/cbi.owl#BiasedAgent"/>
        <exob:isEngagedIn rdf:resource="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#EvaluatingLikelihoodOfDisease"/>
        <rdfs:comment xml:lang="en">A physician involved in the assessment of disease likelihood, potentially affected by cognitive biases such as the base rate fallacy</rdfs:comment>
        <rdfs:label xml:lang="en">Dr Lee</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#EvaluatingLikelihoodOfDisease -->

    <owl:NamedIndividual rdf:about="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#EvaluatingLikelihoodOfDisease">
        <rdf:type rdf:resource="https://w3id.org/framester/data/framestercore/Assessing"/>
        <exob:inActivity rdf:resource="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#MakingDiagnosis"/>
        <exob:isReflectedUponIn rdf:resource="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#TruePositiveObservation"/>
        <rdfs:comment xml:lang="en">The process of determining the probability that a patient has a specific disease based true positive observation.</rdfs:comment>
        <rdfs:label xml:lang="en">Evaluating Likelihood Of Disease</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#InformationRegardingDisease -->

    <owl:NamedIndividual rdf:about="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#InformationRegardingDisease">
        <rdf:type rdf:resource="https://w3id.org/framester/data/framestercore/Information"/>
        <rdfs:comment xml:lang="en">General information about a disease, which includes statistical data and base rates pertinent to its occurrence in a population.</rdfs:comment>
        <rdfs:label xml:lang="en">Information Regarding Disease</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#MakingDiagnosis -->

    <owl:NamedIndividual rdf:about="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#MakingDiagnosis">
        <rdf:type rdf:resource="https://w3id.org/framester/data/framestercore/Deciding"/>
        <exob:hasEngagement rdf:resource="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#EvaluatingLikelihoodOfDisease"/>
        <exob:includes rdf:resource="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#TruePositiveObservation"/>
        <isBasedOn rdf:resource="https://raw.githubusercontent.com/ranacoskun/cognitive_bias_ontology/main/BaseRateFallacy/baseratefallacy.owl#InformationRegardingDisease"/>
        <rdfs:comment xml:lang="en">The decision-making process involved in diagnosing a patient, which considers both base rate information and specific case details.</rdfs:comment>
        <rdfs:label xml:lang="en">Making Diagnosis</rdfs:label>
