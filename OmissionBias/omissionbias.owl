    </owl:ObjectProperty>
    


    <!-- http://ontologydesignpatterns.org/wiki/Submissions:Experience_%26_Observation#includes -->

    <owl:ObjectProperty rdf:about="http://ontologydesignpatterns.org/wiki/Submissions:Experience_%26_Observation#includes">
